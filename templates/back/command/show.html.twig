{% extends 'base.html.twig' %}

{% block title %}Command{% endblock %}

{% block body %}
    <h1>Command</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ command.id }}</td>
            </tr>
            <tr>
                <th>NumFact</th>
                <td>{{ command.numFact }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ command.status }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ command.updatedAt ? command.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreationAt</th>
                <td>{{ command.creationAt ? command.creationAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>TotalTTC</th>
                <td>{{ command.totalTTC }}</td>
            </tr>
            <tr>
                <th>TotalHT</th>
                <td>{{ command.totalHT }}</td>
            </tr>
            <tr>
                <th>TotalTVA</th>
                <td>{{ command.totalTVA }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_command_index') }}">back to list</a>

    <a href="{{ path('back_command_edit', {'id': command.id}) }}">edit</a>

    {{ include('back/command/_delete_form.html.twig') }}
{% endblock %}
